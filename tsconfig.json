{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES5",
    "types" : ["react"],
    "experimentalDecorators": true,
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "lib": [ "es2015", "dom"],
    "strict": true
  },
  "exclude": [
    "node_modules",
  ]
}